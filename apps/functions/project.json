{
  "name": "functions",
  "projectType": "application",
  "sourceRoot": "apps/functions/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/functions"],
      "options": {
        "command": "tsc -p apps/functions/tsconfig.build.json"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p apps/functions/tsconfig.json --noEmit"
      }
    }
  },
  "tags": []
}
